---
import Basic from '../layouts/Basic.astro';
import '../styles/patterns.css';

import TimelineEntry from '../components/TimelineEntry.astro';

const timelineEntries = [
  {
    eyebrow: "2001 to 2005",
    heading: "Maryland Institute College of Art",
    summary: "BFA Graphic Design with minor in Gender Studies/Literature",
    inlineLink: {
      label: "mica.edu",
      url: "https://mica.edu",
      newTab: true,
    }
  },
  {
    eyebrow: "2005 to 2014",
    heading: "Swedish News, Inc.",
    summary: "Art Director and lorem ipsum dolor sit amet, consectetur adipiscing elit.",
    link: {
      url: "https://nordicreach.com",
      newTab: true,
    }
  },
  {
    eyebrow: "Eyebrow/Date",
    heading: "Heading",
    summary: "Summary",
    link: {
      url: "https://danarbello.com",
      newTab: true,
    }
  }
];
---

<Basic>
  {timelineEntries.map((entry) => (
    <TimelineEntry timelineEntry={entry} />
  ))}
</Basic>
